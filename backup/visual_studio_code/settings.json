{
    "vim.handleKeys": {
        "<C-c>": false,
        "<C-v>": false,
        "<C-a>": false,
        "<C-f>": false
    },
    "editor.fontLigatures": true,
    "editor.suggestSelection": "first",
    "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
    "workbench.startupEditor": "none",
    "workbench.colorCustomizations": {
        "tab.activeBackground": "#ffae00"
    },
    "files.autoSave": "afterDelay",
    "files.autoSaveDelay": 500,
    "editor.bracketPairColorization.enabled": true,
    "editor.renderWhitespace": "none",
    "editor.insertSpaces": false,
    "editor.wordWrap": "off",
    "editor.accessibilitySupport": "off",
    "editor.foldingImportsByDefault": true,
    "editor.guides.bracketPairsHorizontal": false,
    "editor.guides.highlightActiveIndentation": false,
    "editor.guides.highlightActiveBracketPair": false,
    "editor.guides.indentation": false,
    "editor.hover.delay": 1000,
    "editor.linkedEditing": true,
    "editor.matchBrackets": "near",
    "editor.mouseWheelZoom": true,
    "editor.renderLineHighlight": "none",
    "editor.scrollbar.vertical": "hidden",
    "editor.lineNumbers": "off",
    "editor.minimap.enabled": false,
    "editor.overviewRulerBorder": false,
    "editor.scrollbar.verticalScrollbarSize": 0,
    "editor.smoothScrolling": true,
    "editor.unfoldOnClickAfterEndOfLine": false,
    "editor.cursorBlinking": "phase",
    "editor.cursorSmoothCaretAnimation": false,
    "editor.cursorSurroundingLines": 10,
    "editor.fontFamily": "Cascadia Code Light, Consolas, 'Courier New', monospace",
    "editor.fontWeight": "bold",
    "workbench.colorTheme": "Solarized Light",
    "editor.suggest.insertMode": "replace",
    "editor.suggest.localityBonus": true,
    "files.eol": "\n",
    "workbench.commandPalette.history": 1000,
    "workbench.statusBar.visible": false,
    "workbench.tips.enabled": false,
    "breadcrumbs.enabled": false,
    "workbench.editor.revealIfOpen": true,
    "debug.showBreakpointsInOverviewRuler": true,
    "code-runner.clearPreviousOutput": true,
    "files.exclude": {
        "**/.classpath": true,
        "**/.project": true,
        "**/.settings": true,
        "**/.factorypath": true
    },
    "explorer.confirmDelete": false,
    "files.enableTrash": false,
    "vim.useSystemClipboard": true,
    "git.autofetch": true,
    "explorer.confirmDragAndDrop": false,
    "vim.hlsearch": true,
    "editor.quickSuggestionsDelay": 0,
    "todo-tree.tree.scanMode": "workspace",
    "todo-tree.highlights.defaultHighlight": {
        "icon": "alert",
        "type": "text-and-comment",
        "foreground": "black",
        "background": "white",
        "opacity": 50,
        "iconColour": "blue",
        "gutterIcon": true,
    },
    "todo-tree.highlights.customHighlight": {
        "TODO": {
            "icon": "report",
            "foreground": "white",
            "background": "green",
            "iconColour": "green",
        },
        "NOTE": {
            "icon": "note",
            "foreground": "white",
            "background": "gray",
            "iconColour": "gray",
        },
        "COMMENT": {
            "icon": "note",
            "foreground": "white",
            "background": "gray",
            "iconColour": "gray",
        },
        "FIXME": {
            "foreground": "black",
            "background": "yellow",
            "iconColour": "yellow",
        },
        "BUG": {
            "foreground": "white",
            "background": "red",
            "iconColour": "red",
        },
        "[ ]": {
            "icon": "check",
            "foreground": "black",
            "background": "yellow",
            "iconColour": "yellow",
        },
        "[x]": {
            "icon": "check",
            "foreground": "white",
            "background": "green",
            "iconColour": "green",
        }
    },
    "todo-tree.general.tags": [
        "BUG",
        "HACK",
        "FIXME",
        "TODO",
        "NOTE",
        "COMMENT",
        "[ ]",
        "[x]"
    ],
    "todo-tree.regex.regex": "(//|#|<!--|;|/\\*|^|^\\s*(-|\\d+.))\\s*($TAGS).*(\\n\\s*//\\s{2,}.*)*",
    "todo-tree.general.enableFileWatcher": true,
    "todo-tree.tree.disableCompactFolders": true,
    "explorer.compactFolders": false,
    "workbench.tree.indent": 30,
    "git.confirmSync": false,
}
